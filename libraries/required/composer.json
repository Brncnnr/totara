{
  "name": "totara/totara_require",
  "description": "Required libraries used in production",
  "type": "metapackage",
  "homepage": "https://www.totaralearning.com",
  "require": {
    "php": ">=7.4.3 <8.2",
    "symfony/polyfill-php81": "1.26.0",
    "symfony/polyfill-php80": "1.26.0",
    "symfony/polyfill-php74": "1.26.0",
    "ezyang/htmlpurifier": "dev-totara-4.15.0 as 4.15.0",
    "phpmailer/phpmailer": "6.5.4",
    "webonyx/graphql-php" : "v14.11.3",
    "box/spout": "dev-totara-3.3.0",
    "simplepie/simplepie": "1.6.0",
    "dompdf/dompdf": "dev-totara-1.2.2",
    "sabberworm/php-css-parser": "dev-totara-8.4.0 as 8.4.0",
    "phpoffice/phpspreadsheet" : "dev-totara-1.23.0",
    "opis/json-schema": "2.3.0",
    "adodb/adodb-php": "5.22.2",
    "michelf/php-markdown": "1.9.1",
    "nyholm/psr7": "1.5.1",
    "lcobucci/jwt": "dev-totara-4.1.5 as 4.1.5",
    "league/oauth2-server": "dev-totara-8.3.5 as 8.3.5",
    "matthiasmullie/minify": "1.3.68",
    "geoip2/geoip2": "2.11.0",
    "totara/xhprof": "dev-totara-2.3.5",
    "goat1000/svggraph": "dev-totara-2.30.0",
    "setasign/fpdi": "dev-totara-1.6.2",
    "wikimedia/less.php": "dev-totara-3.1.0",
    "scssphp/scssphp": "dev-totara-1.0.5",
    "phpseclib/phpseclib": "2.0.37"
  },
  "repositories": [
    {
      "_info": "Fork of lcobucci/jwt and lcobucci/clock required in order to simultanously support PHP 7.3 and 8.0",
      "type": "vcs",
      "url": "https://github.com/totara/jwt"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/xhprof"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/SVGGraph"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/FPDI"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/scssphp"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/dompdf"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/PhpSpreadsheet"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/htmlpurifier"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/less.php"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/spout"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/PHP-CSS-Parser"
    },
    {
      "type": "vcs",
      "url": "https://github.com/totara/oauth2-server"
    }
  ],
  "replace": {
    "lcobucci/clock": "*"
  },
  "config": {
    "vendor-dir": "./",
    "optimize-autoloader": true
  }
}
