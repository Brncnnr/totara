{
  "name": "totara/totara_behat",
  "description": "Totara Behat related code",
  "license": "GPL-3.0-or-later",
  "type": "library",
  "homepage": "https://www.totaralearning.com",
  "require-dev": {
    "behat/mink": "1.10.0",
    "behat/mink-extension": "2.3.1",
    "behat/mink-goutte-driver": "1.3.0",
    "behat/mink-selenium2-driver": "1.6.0",
    "symfony/process": "4.4.41",
    "behat/behat": "3.10.0",
    "guzzlehttp/guzzle": "dev-totara-6.5.5 as 6.5.5"
  },
  "require": {
    "php": ">=7.4.3 <8.2",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-date": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-hash": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-pcre": "*",
    "ext-session": "*",
    "ext-SimpleXML": "*",
    "ext-SPL": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-zip": "*",
    "ext-zlib": "*"
  },
  "repositories": [
    {
      "_info": "Fork of guzzlehttp/guzzle required in order to support PHP 8.1",
      "type": "vcs",
      "url": "https://github.com/totara/guzzle"
    }
  ],
  "autoload": {
    "psr-0": {
      "Moodle\\BehatExtension\\": "extension/src/"
    }
  }
}
